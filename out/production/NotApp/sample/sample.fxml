<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
fx:controller="sample.Controller">
   <bottom>
      <Label prefHeight="44.0" prefWidth="43.0" text="notdev" BorderPane.alignment="CENTER_RIGHT">
         <BorderPane.margin>
            <Insets right="20.0" />
         </BorderPane.margin></Label>
   </bottom>
   <center>
      <VBox alignment="CENTER" prefHeight="325.0" prefWidth="190.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="notapp" textAlignment="JUSTIFY" textFill="#f23030">
               <font>
                  <Font name="Comic Sans MS" size="33.0" />
               </font>
            </Label>
            <Label prefHeight="40.0" prefWidth="132.0" text="Compare appliances and see your savings!" wrapText="true">
               <font>
                  <Font size="11.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Label>
            <Button mnemonicParsing="false" onAction="#getStartedButtonClicked" text="Get Started" textFill="#316cf5">
               <font>
                  <Font name="Comic Sans MS" size="14.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="Tutorial">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin></Button>
            <Button alignment="CENTER" mnemonicParsing="false" text="Exit" />
         </children>
      </VBox>
   </center>
   <top>
      <AnchorPane prefHeight="31.0" prefWidth="600.0" BorderPane.alignment="CENTER" />
   </top>
   <left>
      <AnchorPane prefHeight="325.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <AnchorPane prefHeight="325.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
